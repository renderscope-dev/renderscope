{
  "name": "renderscope-ui",
  "version": "0.1.0",
  "description": "Reusable React components for comparing, benchmarking, and visualizing rendering engines",
  "keywords": [
    "react",
    "rendering",
    "image comparison",
    "benchmarking",
    "computer graphics",
    "renderer comparison",
    "3d graphics"
  ],
  "author": "Ashutosh Mishra",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/renderscope-dev/renderscope",
    "directory": "packages/renderscope-ui"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/theme.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:watch": "rollup -c rollup.config.mjs --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo storybook-static"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28",
    "@rollup/plugin-node-resolve": "^16",
    "@rollup/plugin-typescript": "^12",
    "@storybook/addon-a11y": "^8",
    "@storybook/addon-essentials": "^8",
    "@storybook/blocks": "^8",
    "@storybook/manager-api": "^8",
    "@storybook/react-vite": "^8",
    "@storybook/theming": "^8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "eslint": "^8",
    "postcss": "^8",
    "rollup": "^4",
    "rollup-plugin-dts": "^6",
    "rollup-plugin-peer-deps-external": "^2",
    "rollup-plugin-postcss": "^4",
    "storybook": "^8",
    "tslib": "^2",
    "typescript": "^5",
    "vitest": "^2"
  }
}
